package sorting;

public class BubbleSort {

	public static void main(String[] args) {
		int a[]= {12,2,23,8,19,20,45};
		int n= a.length;
		int temp=0;
		
		
		for(int i=1;i<n;i++) {
			for(int j=0;j<n-i;j++) {
				
				if(a[j]>a[j+1]) {
					 
					temp=a[j+1];
					a[j+1]=a[j];
					 a[j]=temp;
				}
			}
			
		}
		System.out.println("Original array is: ");
		for(int k=0;k<n;k++) {
			System.out.println(a[k]);
		}
		
		
		
	}
	
	
	
	
	
	
	
}
